{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "src": "docs-conceptual",
        "version": "secmgmtopenps-1.0",
        "dest": "secmgmt"
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "docs-conceptual",
        "version": "secmgmtopenps-1.0",
        "dest": "secmgmt/ce"
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "secmgmtopenps-1.0",
        "version": "secmgmtopenps-1.0",
        "exclude": [
          "toc.yml"
        ],
        "dest": "module"
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "secmgmtopenps-1.0",
        "version": "secmgmtopenps-1.0",
        "dest": "module/secmgmtopenps-1.0"
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "secmgmt/breadcrumb"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "virtual-folder",
        "dest": "module"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "versions": {
      "secmgmtopenps-1.0": {
        "dest": "secmgmtopenps-1.0"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "powershell",
      "breadcrumb_path": "/powershell/secmgmt/breadcrumb/toc.json",
      "author": "isaiahwilliams",
      "feedback_github_repo": "microsoftdocs/secmgmt-open-powershell-docs",
      "feedback_system": "GitHub",
      "feedback_product_url": "https://github.com/microsoft/secmgmt-open-powershell/issues/new",
      "ms.author": "iswillia",
      "searchScope": [
        "Microsoft 365",
        "Microsoft 365 PowerShell",
        "Security and Management PowerShell"
      ]
    },
    "fileMetadata": {
	  "ms.prod": {
        "secmgmtopenps-1.0/**/*.*": "powershell",
		"docs-conceptual/**/*.md": "powershell"
      },
      "ms.topic": {
        "secmgmtopenps-1.0/**/*.*": "reference",
		"docs-conceptual/**/*.md": "article"
	    }
	},
    "template": [],
    "dest": "secmgmt-docs-powershell",
    "markdownEngineName": "markdig"
  }
}
